"begin":4853},{"end":5078,"text":"Olympiapark","category":"I-LOC","begin":5067},{"end":5238,"text":"LSB","category":"I-ORG","begin":5235},{"end":5456,"text":"Brandi","category":"I-PER","begin":5450},{"end":5517,"text":"Robert Harting","category":"I-PER","begin":5503},{"end":5575,"text":"Eliteschule","category":"I-ORG","begin":5564},{"end":5681,"text":"LSB","category":"I-ORG","begin":5678},{"end":5774,"text":"Heiner Brandi","category":"I-PER","begin":5761},{"end":5791,"text":"Berliner","category":"I-MISC","begin":5783}]}