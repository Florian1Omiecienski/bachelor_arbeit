ich\" \n\n","named_entities":[{"end":147,"text":"deutschen","category":"I-MISC","begin":138},{"end":180,"text":"EU-Regeln","category":"I-MISC","begin":171},{"end":204,"text":"syrische","category":"I-MISC","begin":196},{"end":231,"text":"Deutschland","category":"I-LOC","begin":220},{"end":354,"text":"Berlin","category":"I-LOC","begin":348},{"end":464,"text":"Schabowski","category":"I-PER","begin":454},{"end":496,"text":"deutsche","category":"I-MISC","begin":488},{"end":633,"text":"Syriens","category":"I-LOC","begin":626},{"end":707,"text":"Bundesrepublik Deutschland","category":"I-LOC","begin":681}]}